{
  "name": "@pixdif/cli",
  "version": "0.9.0",
  "description": "Command-Line Interface of PixDif",
  "type": "module",
  "main": "dist",
  "bin": {
    "pixdif": "pixdif.js"
  },
  "scripts": {
    "build": "tsc",
    "debug": "tsc --incremental --sourceMap",
